{
  "name": "anderson-scherdovski/base",
  "description": "A base",
  "license": "MIT",
  "keywords": [
    "base"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Anderson Scherdovski",
      "email": "aandersonscherdovski@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "laravel/framework": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0|^9.0",
    "phpunit/phpunit": "^8.4|^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "AndersonScherdovski\\Base\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AndersonScherdovski\\Base\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "AndersonScherdovski\\Base\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
